<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="360.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dialog.SignUpController">
   <children>
      <TextField fx:id="accountField" layoutX="226.0" layoutY="51.0" prefHeight="40.0" prefWidth="210.0" promptText="Tài khoản">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="225.0" layoutY="115.0" prefHeight="40.0" prefWidth="210.0" promptText="Mật khẩu">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label layoutX="95.0" layoutY="58.0" text="Tài khoản:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="95.0" layoutY="122.0" text="Mật khẩu:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="signUpButton" layoutX="113.0" layoutY="268.0" mnemonicParsing="false" onAction="#signUp" prefHeight="53.0" prefWidth="112.0" text="Đăng ký">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="302.0" layoutY="268.0" mnemonicParsing="false" onAction="#returnButton" prefHeight="53.0" prefWidth="112.0" text="Quay lại">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="reEnterField" layoutX="225.0" layoutY="180.0" prefHeight="40.0" prefWidth="210.0" promptText="Nhập lại mật khẩu">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label layoutX="26.0" layoutY="187.0" text="Nhập lại mật khẩu:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
